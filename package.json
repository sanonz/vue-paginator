{
  "name": "@sanonz/vue-paginator",
  "version": "1.0.0",
  "description": "vue paginator component",
  "main": "dist/paginator.cjs.js",
  "unpkg": "dist/paginator.umd.min.js",
  "module": "dist/paginator.es.js",
  "scripts": {
    "build": "rollup -c --environment NODE:production && npm run uglify",
    "uglify": "uglifyjs dist/paginator.umd.js --compress --mangle --output dist/paginator.umd.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanonz/vue-paginator.git"
  },
  "keywords": [
    "vue-page",
    "vue-paginate",
    "vue-paginator",
    "vue",
    "vue-router"
  ],
  "author": "Sanonz <sanonz@126.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sanonz/vue-paginator/issues"
  },
  "homepage": "https://github.com/sanonz/vue-paginator#readme",
  "files": [
    "dist/*.js",
    "index.d.ts"
  ],
  "dependencies": {
    "vue": "^2.5.17",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-vue": "^4.3.2",
    "uglify-js": "^3.4.9",
    "vue-template-compiler": "^2.5.17"
  },
  "directories": {
    "example": "examples"
  }
}
